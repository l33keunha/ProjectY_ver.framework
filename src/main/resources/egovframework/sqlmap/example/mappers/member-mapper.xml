<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybs.member.common.service.impl.MemberMapper">
		
		
	<select id="login" parameterType="mVO" resultType="egovMap">
		SELECT 
			NO
			,NAME
			,DEPT
		FROM MEMBERS
		WHERE 1=1
			AND ID = #{id}
			AND PWD = #{pwd}
	</select>
	
</mapper>