<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybs.user.common.service.impl.UserMapper">
		
	<select id="login" parameterType="uVO" resultType="egovMap">
		SELECT 
			UNO
			,NAME
			,DEPT
		FROM USER_INFO
		WHERE 1=1
			AND ID = #{id}
			AND PWD = #{pwd}
	</select>
	
</mapper>